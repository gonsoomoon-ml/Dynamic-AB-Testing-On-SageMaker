    "import numpy as np\n",
    "import scipy.stats as stats \n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.arange (0, 1.001, 0.001)\n",
    "a, b = 2, 2\n",
    "\n",
    "beta_pdf = stats.beta.pdf(x, a, b)\n",
    "\n",
    "print(beta_pdf)"
